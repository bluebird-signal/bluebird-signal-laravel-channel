{
  "name": "bluebird-signal/bluebird-signal-laravel-channel",
  "description": "Laravel Notification driver for BlueBirdSignal",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Treasure Priyamal",
      "email": "treasure@sopien.com"
    }
  ],
  "require": {
    "php": ">=7.1|^8.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/notifications": "^7.0|^8.0",
    "illuminate/support": "^7.0|^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "BlueBirdSignal\\BlueBirdSignalChannel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueBirdSignal\\BlueBirdSignalChannel\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "BlueBirdSignal\\BlueBirdSignalChannel\\BlueBirdSignalServiceProvider"
      ]
    }
  }
}
